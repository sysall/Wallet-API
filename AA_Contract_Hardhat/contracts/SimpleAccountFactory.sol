        return Create2.computeAddress(bytes32(bytes(salt)), keccak256(abi.encodePacked(
                type(ERC1967Proxy).creationCode,
                abi.encode(
                    address(accountImplementation),
                    abi.encodeCall(SimpleAccount.initialize, (owner))
                )
                
            )));

    }
    function executeFun(address owner,uint256 salt, address to, uint256 amount, bytes calldata data) public  {
       
    SimpleAccount val =   SimpleAccount(payable(new ERC1967Proxy{salt : bytes32(salt)}(
                address(accountImplementation),
                abi.encodeCall(SimpleAccount.initialize, (owner))
            )));
           val.execute(to, amount, data);
    }

     
}
 
